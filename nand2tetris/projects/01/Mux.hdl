/** 
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    // Put your code here:
    And(a=sel, b=b, out=sb);
    Not(in=sel, out=sela);
    And(a=sela, b=a, out=sa);
    Or(a=sa, b=sb, out=out);
}